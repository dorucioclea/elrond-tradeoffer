{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "File": {
      "Path": "/var/lib/elrondTradeOffer/app_{0:yyyy}-{0:MM}-{0:dd}.log",
      "Append": true,
      "MinLevel": "Warning", // min level for the file logger
      "FileSizeLimitBytes": 5000000, // use to activate rolling file behaviour
      "MaxRollingFiles": 10 // use to specify max number of log files
    }
  },
  "AllowedHosts": "*",
  "SmartContractAddressDev": "",
  "SmartContractAddressTest": "",
  "SmartContractAddressMain": "",
  "DataProvider": "mysql",
  "MySqlConnectionString": "Server=host.docker.internal; Port=3306; Database=elrondTradeOffer; Uid=foo; Pwd=bar; SslMode=none;",
  "TelegramBotToken": "SECRET-TOKEN",
  "StatusPollInterval": 60000
}
